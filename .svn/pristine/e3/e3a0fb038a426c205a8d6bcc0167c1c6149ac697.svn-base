%pagge-styletop{//继承
     margin-top: 15px;
}
.navigationbar{//头部
    height: 40px; 
    background: #0a435e;
    width: 100%;
    color: #fff;
    line-height: 40px;
    margin-bottom: 10px;
    .navigationname{
        margin-left: 20px;
   }
}
/* 分页 */
.paagenumber{   
    text-align: center;
    bottom: 10px;
    width: 100%; 
    @extend %pagge-styletop; 
}
//菜单栏的高度,

//上传的正方形
.avatar-uploader .el-upload {
    border: 1px dashed #d9d9d9;
    border-radius: 6px;
    cursor: pointer;
    position: relative;
    overflow: hidden;
  }
  .avatar-uploader .el-upload:hover {
    border-color: #409EFF;
  }
  .avatar-uploader-icon {
    font-size: 28px;
    width: 178px;
    height: 178px;
    line-height: 178px;
    text-align: center;
    border:1px solid #8c939d;
    border-radius: 5px;
  }
  .avatar {
    width: 178px;
    height: 178px;
    display: block;
  }
